function lorecode() {
penUp();
moveTo(90, 400);
//This code makes the main duck body and the parameter changes the angle of it.
function   duck (side) {
  penDown();
  penRGB(255, 255, 255, 1);
  turnTo(side);
  moveForward(25);
  arcRight(90, 5);
  arcLeft(90, 20);
  arcLeft(60, 8);
  penRGB(252, 115, 3, 1);
  turnTo(side);
  moveForward(10);
  arcLeft(180, 3);
  moveForward(10);
  penRGB(255, 255, 255, 1);
  arcRight(90, 15);
  arcLeft(90, 25);
  moveForward(5);
  arcLeft(90, 18);
  moveForward(7);
  turnLeft(90);
  moveForward(5);
  penUp();
  move(-50, -21);
  dot(2);
}
turnLeft(90);
//This code makes the foot of the duck it also randomly changes the color slightly.
function duckFoot() {
  penWidth(2);

  penDown();
  penRGB(randomNumber(250, 255), 115, 3, 1);
  moveForward(18);
  moveBackward(8);
  turnRight(45);
  moveForward(8);
  turnLeft(95);
  moveForward(7);
  turnLeft(90);
  moveForward(7);
  turnLeft(95);
  moveForward(7);
  penUp();
}
//the code below moves the pen to were the leg needs to start.
move(30, 45);
show();
penUp();
turnRight(50);
moveForward(10);
turnTo(270);
moveBackward(15);
turnLeft(90);
penUp();
//The randomLeg code moves the leg to were it needs to be with the preameter of cord and cord2. then I put in the Angel preameter Angel changs the angel so that it can be randomised in the call function.
function randomLeg(cord, cord2, Angel) {
  if(randomNumber(1, 10)>3) {moveTo(cord,cord2);
    turnTo(Angel);
    duckFoot();
    penUp();} else {
    }
}
//The RandomLeg will call in the leg or not so that it is random for every leg not just legs or legs
hide();
moveTo(240, 389);
show();
turnTo(90);

moveTo(230, 400);
//Duck right flips the duck code so that it will besomend to the right other then how it is called in facing left. it also has a preameter that can change the angel. there 
function   DuckRight (side) {
  penWidth(1);
  penDown();
  penRGB(255, 255, 255, 1);
  turnTo(side);
  moveForward(25);
  arcLeft(90, 5);
  arcRight(90, 20);
  arcRight(60, 8);
  penRGB(252, 115, 3, 1);
  turnTo(side);
  moveForward(10);
  arcRight(180, 3);
  moveForward(10);
  penRGB(255, 255, 255, 1);
  arcLeft(90, 15);
  arcRight(90, 25);
  moveForward(5);
  arcRight(90, 18);
  moveForward(7);
  turnRight(90);
  moveForward(5);
  penUp();
  move(50, -21);
  dot(2);
}
DuckRight(90);
hide();

// this moves the pen to were it can call in duckRight at 256, 420
penUp();
moveTo(256,420);
turnTo(180);
duckFoot();
moveTo(241, 420);
turnTo(170);
// this calls the duck feet so there are at lest two feet on the duck.
duckFoot();
//the randomLeg calls in all the duck leg on the right leg. it also has the
randomLeg(270,410,randomNumber(170, 200));
randomLeg(240,420,randomNumber(170, 200));
randomLeg(228,412,randomNumber(170, 200));
randomLeg(248,396,randomNumber(0, 5));
//this makes a honks sound after the rest of the code is done.
for (var i = 0; i < 2; i++) {
  playSound ("Honk1.mp3");
}
}



