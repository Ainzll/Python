 // Peter Did This
// Heart Function
function heart(x,y,r,g,b,angle){
penUp();
moveTo(x,y);
penWidth(1);
penRGB(r,g,b);
turnTo(angle);
penDown();
arcRight(215,10);
moveForward(25);
turnRight(95);
moveForward(25);
arcRight(215,10);
penUp();
penWidth(1);
}

